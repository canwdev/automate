    "fs-extra": "^10.0.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "node-localstorage": "^2.1.4",
    "node-ssh": "^6.0.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.6.5",
    "shelljs": "^0.8.3",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.3"
  }
}
